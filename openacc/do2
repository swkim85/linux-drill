

#####################################
# fft2
#####################################
gcc -g -fopenmp -o fft2_gnu  fft2.c -lm -DSIZE=100000
nvc -mp         -o fft2_nvc  fft2.c -lm -DSIZE=100000
nvc -mp -cuda   -o fft2_cuda fft2.c -lm -DSIZE=100000 -Wl,-z,noexecstack
./fft2_gnu > out
./fft2_nvc > out
./fft2_cuda > out

gnuplot fft2.plot



